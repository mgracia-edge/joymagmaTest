{
  "name": "Magma-Nx-Studio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "compile": "npx webpack --config webpack.config.js",
    "compile-watch": "npx webpack --config webpack.config.js --watch",
    "heroku-postbuild": "npm run compile"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7",
              "ie >= 11"
            ]
          }
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@sendgrid/mail": "^6.3.1",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "ftp": "^0.3.10",
    "http-errors": "^1.7.1",
    "maxmind": "^2.12.0",
    "mongoose": "^5.2.12",
    "morgan": "~1.9.0",
    "node-sass": "^4.9.2",
    "node-sass-middleware": "0.11.0",
    "ua-parser-js": "^0.7.19",
    "xml2js": "^0.4.19"
  },
  "engines": {
    "node": "10.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
